<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>025_ServletJspJdbc_WebProject</display-name>
  <display-name>ServletJSPProject</display-name>
  <welcome-file-list>
    <welcome-file>RegistrationForm.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>EmpRegistationServlet</display-name>
    <servlet-name>EmpRegistationServlet</servlet-name>
    <servlet-class>EmpRegistationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmpRegistationServlet</servlet-name>
    <url-pattern>/EmpRegistationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DisplayEmpListServlet</display-name>
    <servlet-name>DisplayEmpListServlet</servlet-name>
    <servlet-class>DisplayEmpListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayEmpListServlet</servlet-name>
    <url-pattern>/DisplayEmpListServlet</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/MyConn</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
</web-app>